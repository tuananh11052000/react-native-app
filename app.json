{
  "expo": {
    "name": "Smai",
    "version": "2.0.0",
    "slug": "Smai",
    "icon": "./assets/icon1.png",
    "notification":{
      "icon":"./assets/iconNotification.png",
      "color": "#FFF",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon":"./assets/iconNotification.png",
          "color": "#ffffff",
          "sounds": ["./local/path/to/mySound.wav", "./local/path/to/myOtherSound.wav"],
          "mode": "production"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splashsmai.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
  },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.smait",
      "buildNumber": "2.0.0"
    },
    "android": {
      "package": "com.smai",
      "versionCode": 2,
      "permissions": ["NOTIFICATIONS", "CAMERA", 
        "CAMERA_ROLL",
         "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"],
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": { 
        "foregroundImage": "./assets/icon1.png",
        "backgroundColor": "#FFF" 
      }
    }
    
  }
}