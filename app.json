    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.smait",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.smai",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconapp.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true
    }
    
  }
}