{
  "expo": {
    "name": "Smai",
    "version": "1.0.0",
    "slug": "Smai",
    "icon": "./assets/iconapp.png",
    "notification":{
      "icon":"./assets/iconapp.png"
    },
    "splash": {
      "image": "./assets/splashsmai.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "updates": {
